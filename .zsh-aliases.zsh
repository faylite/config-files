# Tmux
alias ta='tmux attach -t'
alias tnew='tmux new -s'
alias tls='tmux ls'
alias tkill='tmux kill-session -t'

# Git
alias gadd='git add'
alias gcommit='git commit -m'
alias gpush='git push origin'

# Edit config file shortcuts
alias ev='vim ~/.vimrc'
alias et='vim ~/.tmux.conf'
alias ez='vim ~/.zshrc'

# General
alias c='clear'
